#! /usr/bin/env zsh

# I adapted this from code I didn't save the link for

color=0;
col=4;

while [ $color -lt 256 ]; do
    echo -n -e "$(printf "%03g" $color): \\033[38;5;${color}mhello\\033[48;5;${color}mworld\\033[0m"
    if ((color % col == (col-1))); then
        echo "";
    fi
    ((color++));
done
